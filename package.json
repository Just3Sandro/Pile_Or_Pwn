{
  "name": "stack-visualizer",
  "displayName": "Stack Visualizer",
  "description": "Visualiser graphiquement la pile à partir d'un output.json généré par ton simulateur C.",
  "version": "0.0.1",
  "publisher": "your-name",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "activationEvents": [
    "onCommand:stackVisualizer.open",
    "onCommand:pileOrPwn.executeFile"
  ],
  "contributes": {
    "commands": [
      {
        "command": "stackVisualizer.open",
        "title": "Stack Visualizer: Open"
      },
      {
        "command": "pileOrPwn.executeFile",
        "title": "Pile or Pwn: Exécuter le fichier..."
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "pileOrPwn.executeFile",
          "when": "resourceExtname == .asm || resourceExtname == .s || resourceExtname == .bin",
          "group": "navigation"
        }
      ],
      "editor/title": [
        {
          "command": "pileOrPwn.executeFile",
          "when": "resourceExtname == .asm || resourceExtname == .s || resourceExtname == .bin",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "echo \"No lint configured\"",
    "test": "mocha --recursive --timeout 5000",
    "test:watch": "mocha --watch",
    "test:all": "make || true && npm test"
  },
  "devDependencies": {
    "chai": "^4.5.0",
    "mocha": "^10.8.2",
    "proxyquire": "^2.1.3",
    "sinon": "^15.2.0",
    "vscode": "^1.1.37"
  }
}
